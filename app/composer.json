{
    "name": "tsyren/onlineshop",
    "description": "online-shop",
    "license": "proprietary",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-filter": "*",
        "ext-iconv": "*",
        "doctrine/collections": "^2.2.2",
        "doctrine/dbal": "^3.9.4",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/migrations": "*",
        "doctrine/orm": "*",
        "phpdocumentor/reflection-docblock": "^5.6.1",
        "phpstan/phpdoc-parser": "^2.1.0",
        "symfony/asset": "~7.2.0",
        "symfony/asset-mapper": "~7.2.3",
        "symfony/console": "~7.2.1",
        "symfony/doctrine-messenger": "~7.2.3",
        "symfony/dotenv": "~7.2.0",
        "symfony/expression-language": "~7.2.0",
        "symfony/flex": "^2.5.0",
        "symfony/form": "~7.2.4",
        "symfony/framework-bundle": "~7.2.4",
        "symfony/http-client": "~7.2.4",
        "symfony/http-foundation": "~7.2.3",
        "symfony/http-kernel": "~7.2.4",
        "symfony/intl": "~7.2.0",
        "symfony/mailer": "~7.2.3",
        "symfony/messenger": "~7.2.4",
        "symfony/mime": "~7.2.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/notifier": "~7.2.0",
        "symfony/process": "~7.2.4",
        "symfony/property-access": "~7.2.3",
        "symfony/property-info": "~7.2.3",
        "symfony/routing": "~7.2.3",
        "symfony/runtime": "^7.2",
        "symfony/security-bundle": "~7.2.3",
        "symfony/security-core": "~7.2.3",
        "symfony/security-http": "~7.2.4",
        "symfony/serializer": "~7.2.4",
        "symfony/stimulus-bundle": "^2.23.0",
        "symfony/string": "~7.2.0",
        "symfony/translation": "~7.2.4",
        "symfony/twig-bundle": "~7.2.0",
        "symfony/ux-turbo": "^2.23",
        "symfony/validator": "~7.2.4",
        "symfony/web-link": "~7.2.0",
        "symfony/yaml": "~7.2.3",
        "twig/extra-bundle": "^3.20",
        "twig/twig": "^2.12 || ^3.20"
    },
    "require-dev": {
        "deptrac/deptrac": "^2.0.6",
        "doctrine/doctrine-fixtures-bundle": "^4.0",
        "doctrine/persistence": "^3.4",
        "ergebnis/composer-normalize": "^2.45",
        "friendsofphp/php-cs-fixer": "^3.70",
        "maglnet/composer-require-checker": "^4.16.1",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^2.1.7",
        "phpunit/phpunit": "^9.6.22",
        "rector/rector": "^2.0.10",
        "savinmikhail/add_named_arguments_rector": "^0.1.11",
        "savinmikhail/comments-density": "0.5.9",
        "symfony/browser-kit": "~7.2.4",
        "symfony/css-selector": "~7.2.0",
        "symfony/debug-bundle": "~7.2.0",
        "symfony/maker-bundle": "^1.62.1",
        "symfony/phpunit-bridge": "^7.2",
        "symfony/stopwatch": "~7.2.4",
        "symfony/web-profiler-bundle": "~7.2.4",
        "vimeo/psalm": "^6.8.8"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "php-http/discovery": true,
            "savinmikhail/comments-density": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.2.*"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
